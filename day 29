#include<stdio.h>
void main()
{
struct book
{
char name;
float price;
int pages;
};
struct book b1,b2;
printf("enter books name,price and no. of pages of three books\n");
scanf("%c%f%d",&b1.name,%b1.price,&b1.pages);
scanf("%c%f%d",&b2.name,%b2.price,&b2.pages);
printf("you have entered\n\n");
printf("\nbook name : %c\tprice:%f\tpage no.%d\n\n",b1.name,b1.price,b1.pages);
printf("\nbook name : %c\tprice:%f\tpage no.%d\n\n",b2.name,b2.price,b2.pages);
getch();
}
